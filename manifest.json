{

  "manifest_version": 2,
  "name": "Doc2Code",
  "version": "1.0",

  "description": "Maps the Oracle Java 11+ documentation page to the OpenJDK source code on Github",

  "icons": {
    "48": "icons/logo-48.png",
    "96": "icons/logo-96.png"
  },

  "content_scripts": [
    {
      "matches": ["*://docs.oracle.com/*javase/*"],
      "js": ["main.js"]
    }
  ]

}

